package BasicProCode;

public class IncremnentOperators {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int a=10,x = 5;
		 x=a++;
		System.out.println(x);
		System.out.println(a);
		System.out.println(++x);
		System.out.println(a);
		a%=x;
		System.out.println(a);
	}

}
